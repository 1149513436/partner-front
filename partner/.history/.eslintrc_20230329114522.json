{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    // 在这里添加你需要的规则
    "no-console": "error",
    "no-debugger": "error",
    "no-var": "error",
    "semi": ["error", "always"],
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"]
  }
}



